#include <stdio.h>

int main()
{
   int a[] = {1, 2, 3, 4, 5};
   int *b, i;

    b = a;
    for(i = 0; i < 5; i++)
    {
        printf("%d, ", b[i]);
        b[i] = i*i;
    }
    printf("\n");
    
    for(i = 0; i < 5; i++)
    {
        printf("%d, ", a[i]);
    }
   
   /* 
    1 2 3 4 5
    0 1 4 9 16
    */

    return 0;
}